{
  "manifest_version": 2,

  "name": "FB Post Encryption",
  "description": "This extension Eny/Dec messages in Facebook.",
  "version": "0.1",

  "permissions": [
    "storage"
  ],

  "browser_action": {
    "default_icon": "lockicon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["http://www.facebook.com/*", "https://www.facebook.com/*"],
      "css": ["newStyle.css"],
      "js": ["jquery-2.1.1.min.js", "CryptoJS_AES_SHA256.js", "newScript.js"]
    }
  ]

}